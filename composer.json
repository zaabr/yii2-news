{
  "name": "zaabr/yii2-news",
  "description": "Creating, editing and managing publications of news on the site.",
  "type": "yii2-extension",
  "keywords": ["yii2","extension"],
  "license": "MIT",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Aleksey Zubkov",
      "email": "zaabr@yandex.ru",
      "homepage": "https://github.com/zaabr"
    }
  ],
  "minimum-stability": "stable",
  "require": [{
        "yiisoft/yii2": "~2.0.0",
	"php": ">=7.0.0"
  	}
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/zaabr/yii2-news"
    }
  ],
  "extra": [
  	{
        "bootstrap": "zaabr\\yii2-news\\Bootstrap",
	    "branch-alias": {
    	  "dev-master": "1.0.x"
	    	}
  	}
  ],
  "autoload": {
    "psr-4": {
      "zaabr\\yii2-news\\": "src"
    }
  },
"repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/zaabr/yii2-news.git"
    }
  ]
}
